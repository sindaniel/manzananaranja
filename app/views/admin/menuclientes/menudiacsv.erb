<%- headers = ['Fecha','Proteina','Wok','Sopa','Carbohidrato','Ensalada','Usuario','Direccion','Fecha de creacion'] -%><%= CSV.generate_line headers %>
<%- @menusNombreCliente.each do |menu| -%>
    <%- if menu.protein_id == 0 then proteina = 'No quiere' else proteina = menu.protein.name end %>
    <%- if menu.wok_id == 0 then wok = 'No quiere' else wok = menu.wok.name end %>
    <%- if menu.soup_id == 0 then soup = 'No quiere' else soup = menu.soup.name end %>
    <%- if menu.carbohydrate_id == 0 then carbohydrate = 'No quiere' else carbohydrate = menu.carbohydrate.name end %>
    <%- if menu.salad_id == 0 then salad = 'No quiere' else salad = menu.salad.name end %>
    <%= CSV.generate_line([menu.date,proteina, wok, soup, carbohydrate,salad, menu.firstname+' '+menu.lastname, menu.address, menu.updated_at ]) %><%- end -%>

<%= CSV.generate_line(['Totales']) %>

<%= CSV.generate_line(['WOKS']) %><%- @woks.each do |item| -%><%= CSV.generate_line([item.name, item.total]) %><%- end -%> <%= CSV.generate_line(['Total', @woks.count]) %>
<%= CSV.generate_line(['SOPAS']) %><%- @sopas.each do |item| -%><%= CSV.generate_line([item.name, item.total]) %><%- end -%> <%= CSV.generate_line(['Total', @sopas.count]) %>
<%= CSV.generate_line(['ENSALADAS']) %><%- @ensaladas.each do |item| -%><%= CSV.generate_line([item.name, item.total]) %><%- end -%> <%= CSV.generate_line(['Total', @ensaladas.count]) %>
<%= CSV.generate_line(['CARBOHIDRATOS']) %><%- @carbohidratos.each do |item| -%><%= CSV.generate_line([item.name, item.total]) %><%- end -%> <%= CSV.generate_line(['Total', @carbohidratos.count]) %>



