<div class="container">

  <section class="section featured-product wow fadeInUp">
    <h3 class="tit-inter"><span>Preguntas Frecuentes</span></h3>

    <div class="row box-quehacemos">

      <div class="col-md-5">

            <% 7.times do |i| %>

            <h4><%= @textsGenerals[i].title %></h4>
            <p>
              <%= raw @textsGenerals[i].description %>
            </p>
            <% end %>

      </div>

      <div class="col-md-5 pull-right">


        <% 7.times do |i| %>
            <% j = i+7 %>
            <h4><%= @textsGenerals[j].title %></h4>
            <p>
              <%= raw @textsGenerals[j].description %>
            </p>
        <% end %>



      </div>
    </div>


  </section><!-- /.section -->

</div><!-- /.container -->